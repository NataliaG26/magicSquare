package model;

public class CuadradoMagico {
	
	private int[][] size;
	private String start;
	private String orientation;
	
	public final static 
	
	public CuadradoMagico(int size, String start, String orientation) {
		this.size = new int[size][size];
		this.start = start;
		this.orientation = orientation;
	}

}
